@using KisiselWeb.Models.Classes
@model List<Icon>
@{
    ViewBag.Title = "IconList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<h4 class="mt-2 mb-2">İkonlar</h4>
<table class="table table-bordered">
    <tr>
        <th>Id</th>
        <th>Ikon</th>
        <th>Guncelle</th>
        <th>Sil</th>
    </tr>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.id</td>
            <td ><i class="@x.icon"></i></td>
            <td><a href="/Admin/IconGet/@x.id" class="btn btn-primary">Güncelle</a></td>
            <td><a href="/Admin/IconRemove/@x.id"class="btn btn-danger">Sil</a></td>
        </tr>
    }
</table>
<a href="/Admin/NewIcon/" class="btn btn-primary">Yeni İkon Ekle</a>


